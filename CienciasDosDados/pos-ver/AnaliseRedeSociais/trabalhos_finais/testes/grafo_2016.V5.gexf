      <edge id="497" source="Adriano Baraté" target="Barbara Di Fabio">
        <attvalues>
          <attvalue for="0" value="http://arxiv.org/abs/1602.00739v1" />
          <attvalue for="1" value="Towards a topological fingerprint of music" />
